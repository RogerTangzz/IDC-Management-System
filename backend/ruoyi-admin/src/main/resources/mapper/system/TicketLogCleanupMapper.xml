<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.dynamic.TicketLogCleanupMapper">
    <update id="cleanup" parameterType="java.util.Date">
        delete from biz_ticket_log where create_time &lt; #{value}
    </update>
</mapper>
