# Example env file for frontend

# Basic
VITE_APP_ENV = 'development'
VITE_APP_BASE_API = '/dev-api'
VITE_USE_MOCK = false

# Data permission param alias
# Default is 'mineOnly'. If backend expects a different name (e.g. 'selfOnly'),
# set it here. Frontend will send both the alias and mineOnly=true during
# gray period for compatibility.
VITE_API_MINE_ONLY_PARAM = mineOnly

